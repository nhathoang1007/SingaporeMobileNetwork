<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SpacingLinearLayout">
        <attr name="ll_spacing" format="dimension" />
    </declare-styleable>

    <declare-styleable name="JsShapeView">
        <attr name="js_shape" format="enum">
            <enum name="circle" value="0" />
            <enum name="rect" value="1" />
            <enum name="square" value="2" />
            <enum name="triangle" value="3" />
        </attr>
        <attr name="js_shape_radius" format="dimension" />
        <attr name="js_fill_color" format="color" />
        <attr name="js_stroke_color" format="color" />
        <attr name="js_stroke_width" format="dimension" />
        <attr name="js_shadow_color" format="color" />
        <attr name="js_shadow_size" format="dimension" />
        <attr name="js_shadow_gravity" format="enum">
            <enum name="top" value="0" />
            <enum name="start" value="1" />
            <enum name="end" value="2" />
            <enum name="bottom" value="3" />
        </attr>
    </declare-styleable>
</resources>